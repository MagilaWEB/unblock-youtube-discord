[START]
--wf-tcp=80,443,%DiscordPort%,%GamePort%
--wf-udp=443,%MessengersPort%,19294-19344,50000-50100,%GamePort%

--filter-udp=%MessengersPort%,19294-19344,50000-50100
--filter-l7=discord,stun
--dpi-desync=fake
--dpi-desync-repeats=6
--new

--filter-udp=443
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-tcp=80,443,%MessengersPort%,%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=10000000
%FAKE_TLS%
--new

[youtube]
--filter-tcp=443
%BLOCKLIST%
--ip-id=zero
--dpi-desync=fake
--dpi-desync-repeats=8
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=10000000
%FAKE_TLS%
--new

[discord]
--filter-tcp=%DiscordPort%
--hostlist-domains=discord.media
--dpi-desync=fake
--dpi-desync-repeats=6
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=10000000
%FAKE_TLS%
--new

[messengers]
use_in>>game_mod

[END]
--filter-udp=443
%BLOCKLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-tcp=80,443
%BLOCKLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=16
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=10000000
%FAKE_TLS%
--new

[game_mod]
--filter-udp=%MessengersPort%,%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-autottl=2
--dpi-desync-repeats=10
--dpi-desync-any-protocol=1
%FAKE_UNKNOWN%
--dpi-desync-cutoff=n2
--new
