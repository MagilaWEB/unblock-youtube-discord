[START]
--wf-tcp=80,443,%MessengersPort%,%DiscordPort%,%GamePort%
--wf-udp=443,%MessengersPort%,%MessengersPortUPD%,%DiscordPortUPD%,%GamePort%

--filter-udp=443
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-tcp=80,443,%MessengersPort%,%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=hostfakesplit
--dpi-desync-repeats=4
--dpi-desync-fooling=ts
--dpi-desync-hostfakesplit-mod=host=ozon.ru
--new

[youtube]
--filter-tcp=443
%BLOCKLIST%
--ip-id=zero
--dpi-desync=hostfakesplit
--dpi-desync-repeats=4
--dpi-desync-fooling=ts
--dpi-desync-hostfakesplit-mod=host=www.google.com
--new

[messengers]
use_in>>game_mod

[discord]
--filter-udp=%DiscordPortUPD%
--filter-l7=discord,stun
--dpi-desync=fake
--dpi-desync-repeats=6
--new

--filter-tcp=%DiscordPort%
--hostlist-domains=discord.media
--dpi-desync=hostfakesplit
--dpi-desync-repeats=4
--dpi-desync-fooling=ts
--dpi-desync-hostfakesplit-mod=host=ozon.ru
--new

[END]
--filter-udp=443
%BLOCKLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-tcp=80,443,%MessengersPort%
%BLOCKLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=hostfakesplit
--dpi-desync-repeats=4
--dpi-desync-fooling=ts,md5sig
--dpi-desync-hostfakesplit-mod=host=ozon.ru
--new

[game_mod]
--filter-udp=%MessengersPortUPD%,%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-autottl=2
--dpi-desync-repeats=12
--dpi-desync-any-protocol=1
%FAKE_UNKNOWN%
--dpi-desync-cutoff=n2
--new