[START]
--wf-tcp=80,443,%DiscordPort%,%GamePort%
--wf-udp=443,%MessengersPort%,19294-19344,50000-50100,%GamePort%

--filter-udp=%MessengersPort%,19294-19344,50000-50100
--filter-l7=discord,stun
--dpi-desync=fake
--dpi-desync-repeats=6
--new

--filter-udp=443
%BLOCKLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-l3=ipv4
--filter-tcp=443,%DiscordPort%,%GamePort%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=syndata,multidisorder
--new

--filter-udp=443
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

[messengers]
use_in>>game_mod

[game_mod]
--filter-udp=%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-autottl=2
--dpi-desync-repeats=14
--dpi-desync-any-protocol=1
%FAKE_UNKNOWN%
--dpi-desync-cutoff=n3
--new