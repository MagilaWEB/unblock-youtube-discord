[START]
--wf-tcp=80,443,%MessengersPort%,%DiscordPort%,%GamePort%
--wf-udp=443,%MessengersPort%,%MessengersPortUPD%,%DiscordPortUPD%,%GamePort%

--filter-udp=443
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=16
%FAKE_QUIC%
--new

--filter-tcp=80,443,%MessengersPort%,%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake,multisplit
--dpi-desync-repeats=16
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=1000
%FAKE_TLS%
--new

[youtube]
--filter-tcp=443
%BLOCKLIST%
--ip-id=zero
--dpi-desync=fake,multisplit
--dpi-desync-repeats=16
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=1000
%FAKE_TLS%
--new

[discord]
--filter-udp=%DiscordPortUPD%
--filter-l7=discord,stun
--dpi-desync=fake
--dpi-desync-repeats=16
--new

--filter-tcp=%DiscordPort%
--hostlist-domains=discord.media
--dpi-desync=fake,multisplit
--dpi-desync-repeats=16
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=1000
%FAKE_TLS%
--new

[messengers]
use_in>>game_mod

[END]
--filter-udp=443
%BLOCKLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=16
%FAKE_QUIC%
--new

--filter-tcp=80,443,%MessengersPort%
%BLOCKLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake,multisplit
--dpi-desync-repeats=16
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=1000
%FAKE_TLS%
--new

[game_mod]
--filter-udp=%MessengersPortUPD%,%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
%DOMAINS-EXCLUDE%
--dpi-desync=fake
--dpi-desync-autottl=2
--dpi-desync-repeats=10
--dpi-desync-any-protocol=1
%FAKE_UNKNOWN%
--dpi-desync-cutoff=n2
--new