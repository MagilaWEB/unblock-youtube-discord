[START]
--wf-tcp=80,443,2053,2083,2087,2096,8443,%GameFilter%
--wf-udp=443,19294-19344,50000-65535,%GameFilter%

--filter-udp=443
%BLOCKLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

[discord]
