[START]
--wf-tcp=80,443,%MessengersPort%,%DiscordPort%,%GamePort%
--wf-udp=443,%MessengersPort%,%MessengersPortUPD%,%DiscordPortUPD%,%GamePort%

--filter-udp=443
%BLOCKLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake,hostfakesplit
%FAKE_HOST_DOMAIN%
--dpi-desync-hostfakesplit-midhost=host-4
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-udp=0,%MessengersPortUPD%,%DiscordPortUPD%
--filter-l7=discord,stun
--dpi-desync=fake
%FAKE_DISCORD%
%FAKE_STUN%
--dpi-desync-repeats=6
--new

[discord]
--filter-tcp=%DiscordPort%
--hostlist-domains=discord.media
--dpi-desync=fake
--dpi-desync-repeats=6
--dpi-desync-fooling=ts
%FAKE_TLS_DOMAIN%
--new

[messengers]
use_in>>game_mod

[youtube]
--filter-tcp=443
%BLOCKLIST%
--ip-id=zero
--dpi-desync=fake,hostfakesplit
%FAKE_HOST_DOMAIN%
--dpi-desync-hostfakesplit-midhost=host-4
--dpi-desync-repeats=6
--dpi-desync-fooling=ts
%FAKE_TLS_DOMAIN%
--new

[END]
--filter-tcp=80,443,%MessengersPort%
%BLOCKLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake,hostfakesplit
%FAKE_HOST_DOMAIN%
--dpi-desync-hostfakesplit-midhost=host-4
--dpi-desync-repeats=6
--dpi-desync-fooling=ts
%FAKE_TLS_DOMAIN%
--new

[game_mod]
--filter-udp=443
%IP-SETLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake,hostfakesplit
%FAKE_HOST_DOMAIN%
--dpi-desync-hostfakesplit-midhost=host-4
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-tcp=80,443,%MessengersPort%,%GamePort%
%IP-SETLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake,hostfakesplit
%FAKE_HOST_DOMAIN%
--dpi-desync-hostfakesplit-midhost=host-4
--dpi-desync-repeats=6
--dpi-desync-fooling=ts
%FAKE_TLS_DOMAIN%
--new

--filter-udp=%MessengersPort%,%GamePort%
%IP-SETLIST%
%IP-EXCLUDE%
--dpi-desync=fake
--dpi-desync-autottl=2
--dpi-desync-repeats=12
--dpi-desync-any-protocol=1
%FAKE_UNKNOWN%
--dpi-desync-cutoff=n3
--new
