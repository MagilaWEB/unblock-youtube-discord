[START]
--wf-tcp=80,443,2053,2083,2087,2096,8443,%GameFilter%
--wf-udp=443,19294-19344,50000-50100,%GameFilter%

--filter-udp=19294-19344,50000-50100
--filter-l7=discord,stun
--dpi-desync=fake
--dpi-desync-repeats=6
--new

[discord]
--filter-tcp=2053,2083,2087,2096,8443
--hostlist-domains=discord.media
--dpi-desync=fake,multisplit
--dpi-desync-repeats=16
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=1000
%FAKE_TLS%
--new

[youtube]
--filter-tcp=443
--hostlist-domains=googlevideo.com
--ip-id=zero
--dpi-desync=fake,split2
--dpi-desync-autottl=1
--dpi-desync-repeats=6
--dpi-desync-fooling=badseq
--dpi-desync-badseq-increment=1000
%FAKE_TLS%
--new

[END]
--filter-tcp=80
%BLOCKLIST%
--dpi-desync=fake,split2
--dpi-desync-autottl=1
--dpi-desync-fooling=md5sig
--new

--filter-tcp=443
%BLOCKLIST%
--hostlist-exclude-domains=googlevideo.com
--dpi-desync=syndata,split2
--dpi-desync-fake-syndata="%BINARIES%tls_clienthello_iana_org.bin"
--dpi-desync-ttl=5
--dpi-desync-autottl=1
--dpi-desync-repeats=6
--wssize=1:6
%FAKE_TLS%
--new

--filter-udp=443
%BLOCKLIST%
--dpi-desync=fake
--dpi-desync-repeats=6
%FAKE_QUIC%
--new

--filter-udp=%GameFilter%
%IP-SETLIST%
%DOMAINS-EXCLUDE%
%IP-EXCLUDE%
--dpi-desync=fake
--dpi-desync-autottl=2
--dpi-desync-repeats=12
--dpi-desync-any-protocol=1
%FAKE_UNKNOWN%
--dpi-desync-cutoff=n2
